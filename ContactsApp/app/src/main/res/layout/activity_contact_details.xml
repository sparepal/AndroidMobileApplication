<?xml version="1.0" encoding="utf-8"?>



<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/photo"
            android:layout_width="71dp"
            android:layout_height="94dp"
            app:srcCompat="@drawable/default_image"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="4dp"
            android:layout_marginLeft="157dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="157dp" />

        <TextView
            android:id="@+id/Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="174dp"
            android:layout_marginTop="94dp"
            android:text="Name"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/photo"
            android:layout_marginStart="174dp" />

        <TextView
            android:id="@+id/phone"
            android:layout_width="127dp"
            android:layout_height="18dp"
            android:text="phone "
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="140dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.035" />

        <TextView
            android:id="@+id/email"
            android:layout_width="66dp"
            android:layout_height="23dp"
            android:layout_marginTop="167dp"
            android:text="email "
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.028" />

        <TextView
            android:id="@+id/company"
            android:layout_width="68dp"
            android:layout_height="17dp"
            android:text="company"
            android:layout_marginLeft="9dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="190dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/address"
            android:layout_width="50dp"
            android:layout_height="16dp"
            android:text="address"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="219dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.026" />

        <TextView
            android:id="@+id/birthday"
            android:layout_width="62dp"
            android:layout_height="19dp"
            android:text="birthday"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="248dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/URL"
            android:layout_width="51dp"
            android:layout_height="19dp"
            android:text="URL"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="276dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/fbURl"
            android:layout_width="53dp"
            android:layout_height="21dp"
            android:text="fbURl"
            android:layout_marginLeft="9dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/photo"
            android:layout_marginTop="302dp" />

        <TextView
            android:id="@+id/nickname"
            android:layout_width="66dp"
            android:layout_height="19dp"
            android:text="nickname"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="338dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/twitterURL"
            android:layout_width="77dp"
            android:layout_height="21dp"
            android:text="twitterURL"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="368dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/skype"
            android:layout_width="59dp"
            android:layout_height="20dp"
            android:text="skype"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="394dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/youtube"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="youtube"
            android:layout_marginLeft="1dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="423dp"
            android:layout_marginStart="8dp" />

        <ImageView
            android:id="@+id/edit"
            android:layout_width="37dp"
            android:layout_height="36dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="448dp"
            android:clickable="true"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/edit"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="140dp"
            android:layout_marginRight="190dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintTop_toTopOf="@+id/textView13"
            android:layout_marginTop="27dp"
            app:layout_constraintLeft_toRightOf="@+id/email"
            android:layout_marginLeft="61dp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/company"
            android:layout_marginLeft="60dp"
            app:layout_constraintTop_toTopOf="@+id/textView14"
            android:layout_marginTop="23dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/address"
            android:layout_marginLeft="77dp"
            app:layout_constraintTop_toTopOf="@+id/textView15"
            android:layout_marginTop="29dp"
            android:layout_marginBottom="0dp"
            app:layout_constraintBottom_toTopOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/birthday"
            android:layout_marginLeft="66dp"
            app:layout_constraintTop_toTopOf="@+id/textView16"
            android:layout_marginTop="29dp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="77dp"
            android:layout_marginTop="28dp"
            android:clickable="true"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/URL"
            app:layout_constraintTop_toTopOf="@+id/textView17" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="30dp"
            android:clickable="true"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/fbURl"
            app:layout_constraintTop_toTopOf="@+id/textView18" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/nickname"
            android:layout_marginLeft="62dp"
            app:layout_constraintTop_toTopOf="@+id/textView19"
            android:layout_marginTop="32dp" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="368dp"
            android:clickable="true"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/twitterURL"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="69dp"
            android:layout_marginTop="394dp"
            android:clickable="true"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/skype"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="78dp"
            android:layout_marginTop="423dp"
            android:clickable="true"
            android:text="TextView"
            app:layout_constraintLeft_toRightOf="@+id/youtube"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/delete"
            android:layout_width="42dp"
            android:layout_height="32dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="448dp"
            android:clickable="true"
            app:layout_constraintRight_toLeftOf="@+id/edit"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/delete"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>